{
  "name": "Infographic-Generator",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/JamesMcGuigan/node-archetype.git"
  },
  "licenses": [
    {
      "type": "GPL3",
      "url": "https://github.com/JamesMcGuigan/node-archetype/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "nodemon": "nodemon InfographicGenerator.js",
    "nodemon_quiet": "nodemon InfographicGenerator.js |  grep -v 'nodemon.*:.*\\(___jb_...___\\|.idea/\\|.git/\\)' | uniq",
    "start": "compass compile & compass watch & npm run nodemon",
    "test": "mocha   test/InfographicGeneratorTest.js;",
    "install": "bower install --allow-root;",
    "killall": "killall node ruby",
    "mongo": "mongod",
    "lint": "find ./*.json -exec jsonlint -q {}; ./jslint-all.sh ./app/",
    "compass": "compass compile --force",
    "compass_watch": "compass watch",
    "autosprite": "./app/public/img/sprite/scripts/autosprite.sh           && compass compile --force",
    "autosprite_refresh": "./app/public/img/sprite/scripts/autosprite.sh --refresh && compass compile",
    "compile": "npm run browserify && npm run autosprite",
    "browserify": "node compile_browserify.js",
    "production": "npm run browserify && npm run production_js && npm run production_css",
    "production_js": "./compile_production_js.sh",
    "production_css": "compass compile -e production --force"
  },
  "dependencies": {
    "async": "*",
    "crypto-browserify": "*",
    "body-parser": "*",
    "compression": "*",
    "connect": "*",
    "connect-domain": "*",
    "connect-flash": "*",
    "connect-mongo": "*",
    "cookie-parser": "*",
    "datejs": "*",
    "express": "*",
    "express-error-handler": "*",
    "express-session": "*",
    "hogan.js": "*",
    "http-auth": "*",
    "method-override": "*",
    "mmm": "*",
    "mongojs": "*",
    "morgan": "*",
    "node-uuid": "*",
    "node.extend": "*",
    "process": "*",
    "q": "*",
    "serve-favicon": "*",
    "underscore": "*",
    "unirest": "*",
    "jslint": "~0.6.1"
  },
  "devDependencies": {
    "karma": "*",
    "loadtest": "*",
    "nodemon": "*",
    "mocha": "*",
    "yadda": "*"
  },
  "browserify": {
    "async":             "*",
    "crypto-browserify": "*",
    "node.extend":       "*"
  },
  "global": {
    "jsonlint":   "*",
    "browserify": "*"
  },
  "unload": {
    "mongodb": "1.3.x",
    "ipfilter": "*",
    "mongoose": "3.6.x",
    "mongoose-auto-increment": "2.1.x",
    "mongoose-concrete-timestamps": "0.1.x",
    "multi-node": "0.2.x",
    "jsonpath-rep": "0.0.x"
  }
}
